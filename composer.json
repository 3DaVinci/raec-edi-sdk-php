  "keywords": ["raec","edi","sdk"],
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "3DaVinci",
      "homepage": "https://3davinci.ru"
    },
    {
      "name": "Morozov Andrey",
      "email": "andrey@3davinci.ru"
    }
  ],
  "require": {
